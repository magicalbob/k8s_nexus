{
  "name": "maven-jboss",
  "online": true,
  "storage": {
    "blobStoreName": "default",
    "strictContentTypeValidation": true,
    "writePolicy": "ALLOW"
  },
  "proxy": {
    "remoteUrl": "http://maven.repository.redhat.com/techpreview/all",
    "contentMaxAge": 60,
    "metadataMaxAge": 60
  },
  "negativeCache": {
    "enabled": true,
    "timeToLive": 60
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true,
    "connection": null,
    "authentication": null
  },
  "routingRuleName": null,
  "maven": {
    "versionPolicy": "RELEASE",
    "layoutPolicy": "STRICT",
    "contentDisposition": "INLINE"
  },
  "type": "proxy"
}