{
  "name": "yum-epel",
  "online": true,
  "storage": {
    "blobStoreName": "dwp-s3",
    "strictContentTypeValidation": true
  },
  "proxy": {
    "remoteUrl": "https://dl.fedoraproject.org/pub/epel/",
    "contentMaxAge": 60,
    "metadataMaxAge": 60
  },
  "negativeCache": {
    "enabled": true,
    "timeToLive": 60
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true,
    "connection": {
      "retries": null,
      "userAgentSuffix": null,
      "timeout": null,
      "enableCircularRedirects": false,
      "enableCookies": false,
      "useTrustStore": false
    },
    "authentication": null
  },
  "routingRuleName": null,
  "type": "proxy"
}