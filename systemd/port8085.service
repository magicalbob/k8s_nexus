[Unit]
Description=Kubectl Port Forward 8085
Wants=network-online.target
After=network-online.target

[Service]
User=vagrant
Group=vagrant
Type=simple
ExecStart=/bin/bash -c "kubectl port-forward deployment.apps/nexus -n nexus --address 192.168.0.14 8085:8085"
Restart=always
RestartSec=5s

[Install]
WantedBy=multi-user.target
