{
  "name": "bower-bower",
  "online": true,
  "storage": {
    "blobStoreName": "default",
    "strictContentTypeValidation": true,
    "writePolicy": "ALLOW"
  },
  "proxy": {
    "remoteUrl": "https://registry.bower.io",
    "contentMaxAge": 60,
    "metadataMaxAge": 60
  },
  "negativeCache": {
    "enabled": true,
    "timeToLive": 60
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true,
    "connection": {
      "retries": null,
      "userAgentSuffix": null,
      "timeout": null,
      "enableCircularRedirects": false,
      "enableCookies": false,
      "useTrustStore": false
    },
    "authentication": null
  },
  "routingRuleName": null,
  "bower": {
    "rewritePackageUrls": true
  },
  "type": "proxy"
}